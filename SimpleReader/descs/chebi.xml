<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<name>ChEBI Curation Type System</name>
	<description>Type system for the ChEBI use case of the OpenMinTeD project</description>
	<version>0.2</version>
	<vendor>NaCTeM</vendor>
	
	<types>
		<typeDescription>
			<name>uk.ac.nactem.uima.chebi.ChebiConcept</name>
